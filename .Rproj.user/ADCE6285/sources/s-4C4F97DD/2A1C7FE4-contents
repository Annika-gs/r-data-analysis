#30/05/2018 - Annika Simpson - AHDB pig production figures, used for graph.

library(tidyverse)

Data2 <- read.csv("C:\\Users\\bsavgs\\OneDrive - University of Leeds\\Book3.csv")

ggplot(Data2, aes(x = Quarter, y = Net.Margin), par(las = 2)) +
    xlab("Yearly Quarter") + ylab("Net Margins ?/head") +
    geom_col(colour="seagreen4", fill="seagreen4") +
    theme_classic() +
    theme(axis.text.x = element_text(angle = 45, hjust = 1.1))

